{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "google": true,
    "require": true,
    "module": true,
    "__dirname": true
  },
  "rules": {
    "no-console": 0,
    "react/jsx-uses-vars": 0,
    "linebreak-style": [
      "error",
      "unix"
    ]
  }
}
